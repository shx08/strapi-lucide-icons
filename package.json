{
  "name": "strapi-lucide-icons",
  "version": "0.564.0",
  "description": "Lucide icon picker custom field for Strapi.",
  "license": "MIT",
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "update:lucide-categories": "node scripts/update-lucide-categories.js",
    "update:lucide-release": "node scripts/update-lucide-release.js",
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "@strapi/design-system": "^2.1.2",
    "@strapi/icons": "^2.1.2",
    "lucide-react": "0.564.0",
    "react-intl": "^7.1.13"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.36.0",
    "@strapi/typescript-utils": "^5.35.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.36.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19"
  },
  "strapi": {
    "kind": "plugin",
    "name": "strapi-lucide-icons",
    "displayName": "Lucide Icons",
    "description": "Lucide icon picker custom field for Strapi."
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shx08/strapi-lucide-icons.git"
  },
  "bugs": {
    "url": "https://github.com/shx08/strapi-lucide-icons/issues"
  },
  "homepage": "https://github.com/shx08/strapi-lucide-icons#readme",
  "author": {
    "name": "Hadjiu Sorin",
    "email": "hadjiu.sorin@gmail.com",
    "url": "https://github.com/shx08"
  }
}
